[http]
	emptyAuth = true
[push]
	default = simple
[pull]
	rebase = true
[alias]
    a = add
    aa = add -A
    au = add -u
    b = branch
    c = commit
    ca = commit --amend
    can = commit --amend --no-edit
    cm = commit -m
	co = checkout
    l = log
    ll = log -7 --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    pu = push
    pl = pull
    plr = pull --rebase
	s = status
[user]
	name = Rohan Sanjay
	email = rohanpsanjay@gmail.com
